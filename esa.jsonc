    "buildCommand": "npm run build",
    "assets": {
        "directory": "./frontend/dist",
        "notFoundStrategy": "singlePageApplication"
    },
    "routes": [
        {
            "pattern": "/api/*",
            "function": "api/index"
        }
    ]
}